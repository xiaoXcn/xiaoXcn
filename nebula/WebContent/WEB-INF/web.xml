<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>xiaoxcn</display-name>
  <servlet>
    <servlet-name>handlerServlet</servlet-name>
    <servlet-class>com.nebula.servlet.HandlerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>handlerServlet</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>testServlet</servlet-name>
    <servlet-class>com.nebula.test.TestServlet</servlet-class>
    <init-param>
      <param-name>name</param-name>
      <param-value>zhangsan</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>testServlet</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>indexServlet</servlet-name>
    <servlet-class>com.xiaoxcn.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>indexServlet</servlet-name>
    <url-pattern>/indexServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>queryBookPageServlet</servlet-name>
    <servlet-class>com.xknowledge.servlet.QueryBookPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>queryBookPageServlet</servlet-name>
    <url-pattern>/xknowledge/queryBookPageServlet</url-pattern>
  </servlet-mapping>
</web-app>